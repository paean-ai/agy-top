{
    "name": "agy-top",
    "version": "0.1.5",
    "description": "ASCII-styled usage statistics for Antigravity with leaderboard",
    "author": "Paean AI <dev@paean.ai>",
    "license": "MIT",
    "type": "module",
    "bin": {
        "agy-top": "dist/cli.js"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "bun run tsc",
        "build:watch": "bun run tsc --watch",
        "dev": "bun run src/cli.ts",
        "clean": "rm -rf dist",
        "prepublishOnly": "bun run clean && bun run build",
        "lint": "eslint src/**/*.ts",
        "test": "bun test"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "chalk": "^5.4.1",
        "cli-table3": "^0.6.5",
        "commander": "^13.1.0",
        "conf": "^13.1.0",
        "figures": "^6.1.0",
        "ink": "^6.6.0",
        "ink-spinner": "^5.0.0",
        "log-update": "^7.0.2",
        "open": "^10.1.0",
        "ora": "^8.1.1",
        "react": "^19.2.3"
    },
    "devDependencies": {
        "@types/node": "^22.10.7",
        "@types/react": "^19.2.8",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "vitest": "^3.0.4"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/paean-ai/agy-top.git"
    },
    "keywords": [
        "antigravity",
        "agy",
        "top",
        "usage",
        "statistics",
        "leaderboard",
        "cli",
        "gemini"
    ],
    "homepage": "https://paean.ai",
    "bugs": {
        "url": "https://github.com/paean-ai/agy-top/issues"
    }
}
